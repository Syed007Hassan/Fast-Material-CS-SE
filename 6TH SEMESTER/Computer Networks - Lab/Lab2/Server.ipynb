   "source": [
    "import socket\n",
    "\n",
    "s = socket.socket()\n",
    "print('Socket Created')\n",
    "\n",
    "s.bind(('localhost',8899))\n",
    "\n",
    "s.listen(5)\n",
    "print('Waiting for connections !')\n",
    "\n",
    "\n",
    "while True:\n",
    "    c,addr = s.accept()\n",
    "    print(\"Connected with\",addr)\n",
    "    c.send(bytes('Welcome in socket programming lab','utf-8'))\n",
    "    c.close()"
   ]
